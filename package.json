{
  "name": "types-installer",
  "version": "1.6.3",
  "description": "Installs @types for your existing dependencies",
  "main": "./src/cli.ts",
  "author": "nfour <novus.nfour@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "types",
    "typings",
    "install",
    "cli"
  ],
  "bin": {
    "types-installer": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfour/types-installer"
  },
  "scripts": {
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "coverage": "jest --coverage",
    "release": "cd dist && npm publish",
    "build": "./scripts/build",
    "ig": "rm -rf ./dist && tsc && npm i -g",
    "test": "rm -rf ./dist && tsc",
    "build:windows": "ts-node ./scripts/build.ts",
    "preversion": "yarn lint && yarn build && yarn coverage",
    "preversion:windows": "yarn lint && yarn build:windows && yarn coverage"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "commander": "^5.1.0",
    "execa": "^0.6.3",
    "inquirer": "^3.0.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/typescript": "^2.0.0",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-temando": "^1.5.11",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "3.3"
  }
}
